<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.2.1</Version>
    <Authors>Haiping Chen</Authors>
    <RepositoryUrl>https://github.com/Oceania2018/CherubNLP</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>NLP</PackageTags>
    <PackageLicenseUrl>https://github.com/Oceania2018/CherubNLP/blob/master/LICENSE</PackageLicenseUrl>
    <Copyright>Apache 2.0</Copyright>
    <PackageProjectUrl>https://github.com/SciSharp/CherubNLP</PackageProjectUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>Upgrade dependency.</PackageReleaseNotes>
    <AssemblyVersion>0.2.1.0</AssemblyVersion>
    <FileVersion>0.2.1.0</FileVersion>
    <Company>https://github.com/SciSharp</Company>
    <Description>.NET text analysis tool.</Description>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>DEBUG;TRACE</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Bigtree.Algorithm" Version="0.3.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
    <PackageReference Include="TensorFlow.NET" Version="0.11.2" />
  </ItemGroup>

</Project>
